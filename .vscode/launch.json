    "version": "0.2.0",
    "configurations": [        
        {
            "request": "launch",
            "name": "Launch Server",
            "type": "node",
            "program": "${workspaceFolder}/server.ts",
            "cwd": "${workspaceFolder}",
            "runtimeExecutable": "/home/moru0011/.deno/bin/deno",
            "runtimeArgs": [
                "run",
                "--unstable",
                "--inspect-wait",
                "--allow-all"
            ],
            "attachSimplePort": 9229
        },
        {
            "request": "launch",
            "name": "Launch fwparse",
            "type": "node",
            "program": "${workspaceFolder}/fwparse.ts",
            "cwd": "${workspaceFolder}",
            "runtimeExecutable": "/home/moru0011/.deno/bin/deno",
            "runtimeArgs": [
                "run",
                "--unstable",
                "--inspect-wait",
                "--allow-all"
            ],
            "attachSimplePort": 9229
        },
        {
            "request": "launch",
            "name": "Launch Client",
            "type": "node",
            "program": "${workspaceFolder}/client.ts",
            "cwd": "${workspaceFolder}",
            "runtimeExecutable": "/home/moru0011/.deno/bin/deno",
            "runtimeArgs": [
                "run",
                "--unstable",
                "--inspect=0.0.0.0:9230",
                "--inspect-wait",
                "--allow-all"
            ],
            "attachSimplePort": 9230
        }
    ]
}